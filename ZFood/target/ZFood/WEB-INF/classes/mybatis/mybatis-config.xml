<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
    <settings>
        <!-- 打印查询语句 -->
        <setting name="logImpl" value="STDOUT_LOGGING" />
        <!--二级缓存-->
        <setting name="cacheEnabled" value="true"/>
    </settings>

    <typeAliases>
        <typeAlias alias="Integer" type="java.lang.Integer" />
        <typeAlias alias="Long" type="java.lang.Long" />
        <typeAlias alias="HashMap" type="java.util.HashMap" />
        <typeAlias alias="LinkedHashMap" type="java.util.LinkedHashMap" />
        <typeAlias alias="ArrayList" type="java.util.ArrayList" />
        <typeAlias alias="LinkedList" type="java.util.LinkedList" />
    </typeAliases>
<mappers>
    <!--
    <mapper resource="mybatis/mapper/ClassRoomMapper.xml"/>
    <mapper resource="mybatis/mapper/StudentMapper.xml"/>
    -->
    <mapper resource="mybatis/mapper/AdminHotMapper.xml"/>
    <mapper resource="mybatis/mapper/AdminKindMapper.xml"/>
    <mapper resource="mybatis/mapper/CommentMapper.xml"/>
    <mapper resource="mybatis/mapper/CouponItemMapper.xml"/>
    <mapper resource="mybatis/mapper/CouponMapper.xml"/>
    <mapper resource="mybatis/mapper/CourierMapper.xml"/>
    <mapper resource="mybatis/mapper/FavFoodMapper.xml"/>
    <mapper resource="mybatis/mapper/FavShopMapper.xml"/>
    <mapper resource="mybatis/mapper/FoodMapper.xml"/>
    <mapper resource="mybatis/mapper/OrderItemMapper.xml"/>
    <mapper resource="mybatis/mapper/OrderMapper.xml"/>
    <mapper resource="mybatis/mapper/ShopMapper.xml"/>
    <mapper resource="mybatis/mapper/UserMapper.xml"/>

</mappers>

</configuration>